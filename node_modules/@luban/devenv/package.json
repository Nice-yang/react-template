{
    "name": "@luban/devenv",
    "version": "0.22.0",
    "description": "an opinionated toolset for Lubansoft JS developers",
    "main": "index.js",
    "author": "w1u02",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "http://192.168.3.234/js-dev/devenv.git"
    },
    "bin": {
        "dev-setup": "./bin/dev-setup"
    },
    "scripts": {
        "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
        "build": "yarn tsc",
        "build:clean": "yarn clean && yarn build",
        "build:watch": "yarn tsc -w",
        "mrm:all": "sh ./bin/dev-setup all",
        "mrm:lint": "sh ./bin/dev-setup lint",
        "mrm:git": "sh ./bin/dev-setup git",
        "mrm:typescript": "sh ./bin/dev-setup typescript",
        "mrm:publish": "sh ./bin/dev-setup publish",
        "mrm:commitlint": "sh ./bin/dev-setup commitlint",
        "prepublishOnly": "yarn build:clean",
        "release": "yarn semantic-release --ci false"
    },
    "dependencies": {
        "mrm": "^1.2.2",
        "mrm-core": "^3.3.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@luban/linter": "^1.9.1",
        "@semantic-release/changelog": "^3.0.4",
        "@semantic-release/git": "^7.0.16",
        "@types/node": "^10",
        "husky": "^3.0.5",
        "lint-staged": "^9.2.5",
        "semantic-release": "^15.13.24",
        "typescript": "^3.6.3"
    },
    "publishConfig": {
        "registry": "http://192.168.13.238:8081/repository/npm-luban/"
    }
}
