#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/sh" ]; then
  "$basedir/sh"  "$basedir/../@luban/linter/bin/fix" "$@"
  ret=$?
else 
  sh  "$basedir/../@luban/linter/bin/fix" "$@"
  ret=$?
fi
exit $ret
